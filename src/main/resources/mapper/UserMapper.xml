<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="note.recordAndShare.mapper.UserMapper">
    <select id="selAllUser" resultType="note.recordAndShare.entity.dto.UserDto">
        select u.*, r.rname
        from user u
                 join role r on u.role_id = r.rid
    </select>
</mapper>
